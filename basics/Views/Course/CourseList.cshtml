@model List<Course>


<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <h1> Course List Page</h1>
    <p>
        @foreach (var COURSE in Model)
        {


            <div id="course_@COURSE.Id">
                <h2>@COURSE.Title</h2>
                <p>@COURSE.Description</p>

            </div>
        }
    </p>
</body>
</html>